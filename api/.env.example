APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

LLM_API_URL=http://localhost:11434

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Embedding Service
EMBEDDING_PROVIDER=local
EMBEDDING_LOCAL_URL=http://127.0.0.1:1234/api/embeddings
EMBEDDING_REMOTE_URL=https://api.openai.com
EMBEDDING_API_KEY=
EMBEDDING_BATCH_SIZE=10
EMBEDDING_MAX_RETRIES=3
EMBEDDING_RETRY_DELAY=1
EMBEDDING_MODEL=text-embedding-nomic-embed-text-v1.5

# Search Configuration
SEARCH_SEMANTIC_WEIGHT=0.7
SEARCH_LEXICAL_WEIGHT=0.3
SEARCH_MAX_CHUNKS=100
SEARCH_RERANK_TOP_K=10
SEARCH_ENABLE_RERANKING=false

# Chat Context Configuration
CHAT_CONTEXT_ENABLED=true
CHAT_CONTEXT_LIMIT=10
CHAT_CONTEXT_MAX_TOKENS=30000

# Chat Streaming Configuration
CHAT_STREAMING_ENABLED=true

# LLM Provider Configuration
LLM_DEFAULT_PROVIDER=llm_studio

# LLM Studio Configuration
LLM_STUDIO_MODEL=gemma-3-1b-it-qat
LLM_STUDIO_MAX_TOKENS=30000
LLM_STUDIO_TIMEOUT=120

# Ollama Configuration
LLM_DEFAULT_MODEL=llama2
OLLAMA_CONTEXT_FORMAT=conversational
OLLAMA_TIMEOUT=100
